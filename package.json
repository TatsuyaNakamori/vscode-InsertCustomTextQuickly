{
    "name": "insertcustomtext",
    "displayName": "Insert Custom Text Quickly",
    "description": "Quickly insert custom text",
    "icon": "resources/icon/extension_main.png",
    "version": "1.0.0",
    "publisher": "TatsuyaNakamori",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "keywords": [
        "shortcuts",
        "string",
        "text",
        "insert",
        "snippets"
    ],
    "homepage": "https://github.com/TatsuyaNakamori/vscode-InsertCustomTextQuickly",
    "repository": {
        "type": "git",
        "url": "https://github.com/TatsuyaNakamori/vscode-InsertCustomTextQuickly.git"
    },
    "bugs": {
        "url": "https://github.com/TatsuyaNakamori/vscode-InsertCustomTextQuickly/issues"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "category": "InsertCustomTextQuickly",
                "command": "insertcustomtext.inserttext",
                "title": "%inserttext.title%",
                "when": "editorTextFocus"
            }
        ],
        "configuration": [
            {
                "title": "Insert Custom Text",
                "properties": {
                    "text.01": {
                        "type": "string",
                        "default": "\"Hello!\",„Äé„Åä„ÇÑ„Åô„Åø„Äèüò™üí§, 'Wake up!'"
                    },
                    "text.02": {
                        "type": "string",
                        "default": "Backslash:\\b,\\nNewLine,\\nTAB\\tTAB"
                    },
                    "text.03": {
                        "type": "string",
                        "default": "if (${1:condision}) {\n    $0\n}"
                    },
                    "text.04": {
                        "type": "string",
                        "default": "${UUID}"
                    },
                    "text.05": {
                        "type": "string",
                        "default": "<div>${[HOLD]}</div>"
                    },
                    "text.06": {
                        "type": "string",
                        "default": "<div align=\"${1:left}\">${[HOLD]}</div>"
                    },
                    "text.07": {
                        "type": "string",
                        "default": "[default] ${[DATE]}\\n[ja-format] ${[DATE]YYYYÂπ¥MMMMDÊó•(dd), A h:mm:ja}"
                    },
                    "text.08": {
                        "type": "string",
                        "default": "${[SEQ]%d:1:1}"
                    },
                    "text.09": {
                        "type": "string",
                        "default": "${[SEQ]%c:65:1}"
                    },
                    "text.10": {
                        "type": "string",
                        "default": "${[SEQ]%d:x:x}"
                    }
                }
            }
        ],
        "keybindings": [
            {
                "command": "insertcustomtext.inserttext",
                "key": "ctrl+alt+i",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "group": "1_modification",
                    "command": "insertcustomtext.inserttext",
                    "when": "editorTextFocus"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/voca": "^1.4.0",
        "@types/vscode": "^1.53.0",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    },
    "dependencies": {
        "dayjs": "^1.10.4",
        "voca": "^1.4.0"
    }
}
